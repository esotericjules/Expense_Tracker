  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel src --out-dir build --extensions \".js,.ts,\" --source-maps",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && ts-node src/index.ts",
    "dev": "npm run build && nodemon src/index.ts",
    "seeders": "node src/database/seeders/runSeeders.mjs",
    "generate": "find src/database/seeders -name 'generate*.mjs' -exec node {} \\;",
    "migrations": "node src/database/migrations/runMigrations.mjs",
    "format": "prettier --write .",
    "lint": "eslint --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
